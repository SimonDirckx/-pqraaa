#ifndef EIGEN_ALT_QR_MODULE_H
#define EIGEN_ALT_QR_MODULE_H

#include <Eigen/Core>

#include <PQRAAA/linalg/Eigen-alt/Cholesky>
#include <Eigen/Jacobi>
#include <Eigen/Householder>

#include <Eigen/src/Core/util/DisableStupidWarnings.h>

/** \defgroup QR_Module QR module
  *
  *
  *
  * This module provides various QR decompositions
  * This module also provides some MatrixBase methods, including:
  *  - MatrixBase::householderQr()
  *  - MatrixBase::colPivHouseholderQr()
  *  - MatrixBase::fullPivHouseholderQr()
  *
  * \code
  * #include <Eigen/QR>
  * \endcode
  */

#include <Eigen/src/QR/HouseholderQR.h>
#include <Eigen/src/QR/FullPivHouseholderQR.h>
#include <Eigen/src/QR/ColPivHouseholderQR.h>
#include <Eigen/src/QR/CompleteOrthogonalDecomposition.h>
#ifdef EIGEN_USE_LAPACKE
  #ifdef EIGEN_USE_MKL
    #include "mkl_lapacke.h"
  #else
    #ifdef BEACHPACK_USE_ALT_LAPACK_BINDINGS
      // These are also used in Boost.Numeric.Bindings and the definitions have to match
      #define lapack_complex_float std::complex<float>
      #define lapack_complex_double std::complex<double>
    #endif
    // Alternative to Eigen/src/misc/lapacke.h without lapack-bindings
    #include <PQRAAA/linalg/Eigen-alt/lapacke.h>
  #endif
#include <Eigen/src/QR/HouseholderQR_LAPACKE.h>
#include <Eigen/src/QR/ColPivHouseholderQR_LAPACKE.h>
#endif

#include <Eigen/src/Core/util/ReenableStupidWarnings.h>

#endif // EIGEN_ALT_QR_MODULE_H
